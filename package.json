  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "axios": "^0.21.4",
    "config": "^3.3.6",
    "google-trends-api": "^4.9.2",
    "graphql": "^15.6.0",
    "graphql-scalars": "^1.10.1",
    "materialize-css": "^1.0.0-rc.2",
    "nodemon": "^2.0.13",
    "react-load-script": "0.0.6"
  }
}
